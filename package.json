{
  "name": "@ahdesigns/kindly",
  "description": "my configuration helpers",
  "version": "0.0.4",
  "author": "Adam Hall @AHDesigns",
  "bin": {
    "kindly": "./bin/run"
  },
  "bugs": "https://github.com/AHDesigns/kindly/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "fs-extra": "^9.0.1",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@types/fs-extra": "^9.0.4",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.10",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "globby": "^10",
    "jest": "^26.6.3",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.4",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/AHDesigns/kindly",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "kindly",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "AHDesigns/kindly",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest",
    "test": "jest",
    "lint:ts": "eslint --ext ts ./src",
    "lint:tsc": "tsc --noEmit",
    "lint": "yarn lint:ts && yarn lint:tsc",
    "start": "./bin/run",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
